{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "paths": {
      "@oxy-space/app/*": [
        "../../packages/app/*"
      ],
      "@oxy-space/api/*": [
        "../../packages/api/*"
      ],
      "@oxy-space/ui/*": [
        "../../packages/ui/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}